algoritmo "71"
var
   n:vetor[0..7] de inteiro
   c:inteiro
inicio
  escreval("Vetor de 8 posições")
  escreval("----------------------------------------")
  para c<-0 ate 7 faca
    n[c]<-999
    escreva(n[c],"|")
  fimpara
  escreval()
  escreval("----------------------------------------")


fimalgoritmo