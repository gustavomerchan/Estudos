algoritmo "88"
var
   c:inteiro
   procedimento Gerador(frase:caractere;vezes:inteiro)
   inicio
       escreval("+-------=======-------+")
       para c<-1 ate vezes faca
       escreval(frase)
       fimpara
       escreval("+-------=======-------+")
   fimprocedimento

inicio
   Gerador("Aprendendo portugol",3)
fimalgoritmo