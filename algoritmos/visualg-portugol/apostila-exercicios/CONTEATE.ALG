algoritmo "conteAte"
var
 cont,num:inteiro
inicio
  cont <- 0
  escreval("=======================")
  escreval("CONTADOR")
  escreval("=======================")
  escreva("Quer contar até quanto? ")
  leia(num)
  enquanto cont < num faca
    cont<- cont +1
    escreva(cont)
  fimenquanto
  escreva(" Acabou!")


fimalgoritmo