algoritmo "semnome"
var
 num,soma:real
inicio
   soma<-0
   escreval("----------------")
   escreval("1111")
   escreval("----------------")
   enquanto num<> 1111 faca
     escreva("Digite um número:")
     leia(num)
     soma<-soma+num
  fimenquanto
  escreval("A soma dos valores digitados é",soma)
     
     

fimalgoritmo